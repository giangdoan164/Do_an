<?xml version="1.0"?>
<data>
	<!-- type= "radio,text,checkbox,select -->
	<!-- value_not_space="true" :  Thong tin nhap vào chuyen thanh viet lien -->
	<attribute required="true" type="select" name="type">
		<value><![CDATA[Tạo nút checkbox]]></value>
		<child name="TextboxName"><![CDATA[Tạo textbox tên người]]></child>
		<child name="TextboxMoney"><![CDATA[Tạo textbox nhập tiền]]></child>
		<child name="Textbox"><![CDATA[Tạo đối tượng kiểu textbox]]></child>
		<child name="TextboxDate"><![CDATA[Tạo textbox nhập ngày]]></child>
		<child name="DropDownList"><![CDATA[Tạo DropDown List (Selectbox)]]></child>
		<child name="RadioButton"><![CDATA[Tạo nút Radio]]></child>
		<child name="Textarea"><![CDATA[Tạo textarea]]></child>
		<child name="Button"><![CDATA[Tạo DropDown List (Selectbox)]]></child>
		<child name="Checkbox"><![CDATA[Tạo nút]]></child>
	</attribute>
	<attribute required="true" type="textbox" name="id" value_not_space="true"><![CDATA[ID của đối tượng]]></attribute>
	<attribute required="true" type="textbox" name="name" value_not_space="true" ><![CDATA[Tên của đối tượng]]></attribute>
	<attribute type="radio" name="allownull">
		<value><![CDATA[Cho phép để trống hay không]]></value>
		<child name="yes" value="yes">yes</child>
		<child name="no" value="no">no</child>
	</attribute>
	<attribute required="true" type="select" name="validate">
		<value><![CDATA[Kiểu xác thực dữ liệu)]]></value>
		<child name="text"><![CDATA[]]></child>
		<child name="ddli"><![CDATA[Dữ liệu kiểu xâu]]></child>
		<child name="numberString"><![CDATA[Dữ liệu kiểu selectbox]]></child>
		<child name="phone"><![CDATA[số điện thoại]]></child>
		<child name="date"><![CDATA[ngày tháng]]></child>
		<child name="number"><![CDATA[Dữ liệu kiểu số]]></child>
	</attribute>
	<attribute required="true" type="textbox" name="label"><![CDATA[Nhãn ghi bên trái đối tượng]]></attribute>
	<attribute type="textbox" name="defaul_value"><![CDATA[Giá trị mặc định]]></attribute>
	<attribute type="textbox" name="src_file"><![CDATA[Đường dẫn đến file XML tạo dữ liệu cho đối tượng (dùng cho DropDownList)]]></attribute>
	<attribute type="textbox" name="doc"><![CDATA[Xác định là tài liệu đính kèm của hồ sơ (nhận giá trị "yes"/"no")]]></attribute>
	<attribute type="textbox" name="value"><![CDATA[Giá trị của đối tượng]]></attribute>
	<attribute type="textbox" name="size"><![CDATA[Giá trị của đối tượng]]></attribute>
</data>